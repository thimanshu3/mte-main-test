<html>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <script src='https://cdn.tailwindcss.com'></script>

    <style>
      * { font-size: 11px; line-height: 15px; }
    </style>
  </head>

  <body>
    <div class='w-full border border-black' style='width: 210mm'>
      <div class='flex w-full justify-center border-black'>
        <h4>EXPORT PACKING LIST</h4>
      </div>

      <div class='flex w-full justify-center border-t border-black'>
        <h4>SUPPLY MEANT FOR EXPORT UNDER {{invoiceType}}</h4>
      </div>

      {{#if isLUT}}
        <div
          class='mx-auto grid grid-cols-4 justify-center border-t border-black'
        >
          <div class='col-span-2 border-r border-black'>
            <div class='w-full border-b border-black'>
              <h6 class='font-bold underline'>EXPORTER</h6>
              <p>{{exporterDetails}}</p>
              <p>{{exporterDetailsAddress}}</p>
            </div>
            <div>
              <div>
                <h6 class='border-b border-black font-bold'>CONSIGNEE</h6>
              </div>
              <div class=''>
                <p>{{customerAddress}}</p>
              </div>
            </div>
          </div>
          <div class='col-span-2 border-black'>
            <div class='border-b border-black'>
              <table>
                <tbody>
                  <tr>
                    <td>INVOICE NO</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{id}}</td>
                  </tr>
                  <tr>
                    <td>INVOICE DATE</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td class='px-1'>{{date}}</td>
                  </tr>
                  <tr>
                    <td>LUT NO</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{LUT}}</td>
                  </tr>
                  <tr>
                    <td>IEC CODE</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{IECcode}}</td>
                  </tr>
                  <tr>
                    <td>GSTIN</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>08AICPM6265B1ZF</td>
                  </tr>
                  <tr>
                    <td>{{currency}} @</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{conversionRate}}</td>
                  </tr>
                </tbody>
              </table>
            </div>
            <div class='border-black'>
              <div class='border-b border-black'>
                <h6 class='font-bold'>NOTIFY PARTY</h6>
              </div>
              <div class=''>
                {{#if notifyName2}}
                  <h6>{{notifyName1}}</h6>
                  <p>{{notifyAddress1}}</p>
                  <h6>{{notifyName2}}</h6>
                  <p>{{notifyAddress2}}</p>
                {{else}}
                  <h6>{{notifyName1}}</h6>
                  <p>{{notifyAddress1}}</p>
                {{/if}}
              </div>
            </div>
          </div>
        </div>
        <div class='grid grid-cols-4 border-t border-black'>
          <div class='col-span-2 border-r border-black'>
            <div class='border-b border-black'>
              <h6 class='border-b border-black font-bold'>PORT DETAILS</h6>
              <p>
                PORT OF LOADING :
                {{portOfLoading}}
                <br />
                PORT OF DISCHARGE :
                {{portOfDischarge}}
              </p>
            </div>
            <div>
              <p>
                DELIVERY : 30 DAYS AFTER RECEIVING 100%
                <br />
                PAYMENT : 100% ADVANCE
              </p>
            </div>
          </div>
          <div class='col-span-2 border-black'>
            <div class='border-black'>
              <h6 class='border-b border-black font-bold'>BANK DETAILS</h6>
              <table>
                <tbody>
                  <tr>
                    <td style='white-space: nowrap'>BANK DETAILS</td>
                    <td>:</td>
                    <td style='white-space: nowrap'>
                      J P MORGAN CHASE BANK, NEW YORK
                    </td>
                  </tr>
                  <tr>
                    <td>SWIFT</td>
                    <td>:</td>
                    <td>CHASUS33XXX</td>
                  </tr>
                  <tr>
                    <td style='white-space: nowrap'>FEDWIRE ROUTING NO.</td>
                    <td>:</td>
                    <td>021000021</td>
                  </tr>
                  <tr>
                    <td>CREDIT TO</td>
                    <td>:</td>
                    <td>ICICI BANK, UDAIPUR</td>
                  </tr>
                  <tr>
                    <td>SWIFT</td>
                    <td>:</td>
                    <td>ICICINBBCTS</td>
                  </tr>
                  <tr>
                    <td>A/C NAME</td>
                    <td>:</td>
                    <td>MEWAR TRADERS & ENGINEERS</td>
                  </tr>
                  <tr>
                    <td>A/C NO.</td>
                    <td>:</td>
                    <td>430405500101</td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>

        <div class='grid-cols-24 grid w-full flex-col border-black'>
          <table style='width: 210mm; table-layout: fixed'>
            <thead>
              <tr>
                <th class='border-t border-black' style='width: 7%'>S.NO</th>
                <th class='border border-black' style='width: 20%'>
                  DESCRIPTION OF GOODS
                </th>
                <th class='border border-black' style='width: 6%'>QTY</th>
                <th class='border border-black' style='width: 6%'>UOM</th>
                <th class='border border-black' style='width: 10%'>
                  PACK NO.
                  <br />
                </th>
                <th class='border border-black' style='width: 10%'>
                  PAKING NO. AS PER SIMPOLO
                  <br />
                </th>
                <th class='border border-black' style='width: 12%'>
                  NO. OF PAC / PALLET
                </th>
                <th class='border border-black' style='width: 12%'>
                  WEIGHT DETAILS
                </th>
                <th class='border border-black' style='width: 12%'>WEIGHT</th>
                <th class='border border-black' style='width: 10%'>TRUCK NO.</th>
                <th class='border border-black' style='width: 10%'>CNTR NO.</th>
                <th class='border border-black' style='width: 12%'>ORIGIN</th>
                {{!-- <th class='border-t border-black' style='width: 9%'>PO REF</th> --}}
              </tr>
            </thead>
            <tbody>
              {{#each items}}
                <tr>
                  <td class='border-b border-t border-black'>{{i}}</td>
                  <td
                    class='border border-black'
                    style='white-space: normal; word-wrap: break-word'
                  >
                    {{description}}
                  </td>
                  <td
                    class='border border-black text-center'
                    style='white-space: normal; word-wrap: break-word'
                  >
                    {{quantity}}
                  </td>
                  <td class='border border-black text-center'>{{uom}}</td>
                  <td
                    class='border border-black text-center'
                  >{{packNumber}}</td>
                  <td class='border border-black text-center'>
                    {{packingNumberAsPerSimpolo}}
                  </td>
                  <td
                    class='border border-black text-center'
                  >{{numberOfPack}}</td>
                  <td
                    class='border border-black text-center'
                  >{{weightDetails}}</td>
                  <td
                    class='border border-black text-center'
                    style='white-space: normal; word-wrap: break-word'
                  >
                    {{weight}}
                  </td>
                  <td
                    class='border border-black text-center'
                    style='white-space: normal; word-wrap: break-word'
                  >
                    {{truckNumber}}
                  </td>
                  <td
                    class='border border-black text-center'
                  >{{cntrNumber}}</td>
                  <td class='border border-black text-center'>
                    {{countryOfOrigin}}
                  </td>
                  {{!-- <td
                    class='border-b border-t border-black'
                    style='white-space: normal; word-wrap: break-word'
                  >
                    {{POrefId}}
                  </td> --}}
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>

        <div class='grid grid-cols-9'>
          <table class='col-start-2 col-end-5' style='margin-right: 7px'>
            <thead>
              <tr>
                <th class='border-l border-black'>TOTAL PACKAGES</th>
                <td class='border-l border-r border-black text-center'>
                  {{totalPackages}}
                </td>
              </tr>
              <tr>
                <th class='border border-black'>TOTAL NET WEIGHT</th>
                <td class='border border-black text-center'>
                  {{totalNetWeight}}
                </td>
              </tr>
              <tr>
                <th class='border border-black'>TOTAL GROSS WEIGHT</th>
                <td class='border border-black text-center'>
                  {{totalGrossWeight}}
                </td>
              </tr>
              <tr>
                <th class='border border-black'>TOTAL CBM</th>
                <td class='border border-black text-center'>{{totalCbm}}</td>
              </tr>
            </thead>
          </table>
          <table class='col-start-6 col-end-9' style='margin-right: 7px'>
            <thead>
              <tr>
                <th class='border-l border-black'>CNTR NUMBER</th>
                <td class='border-l border-r border-black text-center'>
                  {{cntrNumber}}
                </td>
              </tr>
              <tr>
                <th class='border border-black'>TRUCK NUMBER</th>
                <td class='border border-black text-center'>{{truckNumber}}</td>
              </tr>
              <tr>
                <th class='border border-black'>LINE SEAL NO.</th>
                <td class='border border-black text-center'>
                  {{lineSealNumber}}
                </td>
              </tr>
              <tr>
                <th class='border border-black'>RFID SEAL NO.</th>
                <td class='border border-black text-center'>
                  {{rfidSealNumber}}
                </td>
              </tr>
              <tr>
                <th class='border border-black'>CNTR SIZE</th>
                <td class='border border-black text-center'>{{cntrSize}}</td>
              </tr>
            </thead>
          </table>
        </div>

      {{else}}
        <div
          class='fmx-auto grid grid-cols-4 justify-center border-t border-black'
        >
          <div class='col-span-2 border-r border-black'>
            <div class='w-full border-b border-black'>
              <h6 class='font-bold underline'>EXPORTER</h6>
              <p>{{exporterDetails}}</p>
              <p>{{exporterDetailsAddress}}</p>
            </div>
            <div>
              <div>
                <h6 class='border-b border-black font-bold'>CONSIGNEE</h6>
              </div>
              <div class=''>
                <p>{{customerAddress}}</p>
              </div>
            </div>
          </div>
          <div class='col-span-2 border-black'>
            <div class='border-b border-black'>
              <table>
                <tbody>
                  <tr>
                    <td>INVOICE NO</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{id}}</td>
                  </tr>
                  <tr>
                    <td>INVOICE DATE</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{date}}</td>
                  </tr>
                  <tr>
                    <td>LUT NO</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{LUT}}</td>
                  </tr>
                  <tr>
                    <td>IEC CODE</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{IECcode}}</td>
                  </tr>
                  <tr>
                    <td>GSTIN</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>08AICPM6265B1ZF</td>
                  </tr>
                  <tr>
                    <td>{{currency}} @</td>
                    <td>&nbsp;&nbsp;&nbsp;:</td>
                    <td>{{conversionRate}}</td>
                  </tr>
                </tbody>
              </table>
            </div>
            <div class='border-black'>
              <div class='border-b border-black'>
                <h6 class='font-bold'>NOTIFY PARTY</h6>
              </div>
              <div class=''>
                {{#if notifyName2}}
                  <h6>{{notifyName1}}</h6>
                  <p>{{notifyAddress1}}</p>
                  <h6>{{notifyName2}}</h6>
                  <p>{{notifyAddress2}}</p>
                {{else}}
                  <h6>{{notifyName1}}</h6>
                  <p>{{notifyAddress1}}</p>
                {{/if}}
              </div>
            </div>
          </div>
        </div>

        <div class='grid grid-cols-4 border-t border-black'>
          <div class='col-span-2 border-r border-black'>
            <div class='border-b border-black'>
              <h6 class='border-b border-black font-bold'>PORT DETAILS</h6>
              <p>
                PORT OF LOADING :
                {{portOfLoading}}
                <br />
                PORT OF DISCHARGE :
                {{portOfDischarge}}
              </p>
            </div>
            <div>
              <p>
                DELIVERY : 30 DAYS AFTER RECEIVING 100%
                <br />
                PAYMENT : 100% ADVANCE
              </p>
            </div>
          </div>
          <div class='col-span-2 border-black'>
            <div class='border-black'>
              <h6 class='border-b border-black font-bold'>BANK DETAILS</h6>
              <table>
                <tbody>
                  <tr>
                    <td style='white-space: nowrap'>BANK DETAILS</td>
                    <td>:</td>
                    <td style='white-space: nowrap'>
                      J P MORGAN CHASE BANK, NEW YORK
                    </td>
                  </tr>
                  <tr>
                    <td>SWIFT</td>
                    <td>:</td>
                    <td>CHASUS33XXX</td>
                  </tr>
                  <tr>
                    <td style='white-space: nowrap'>FEDWIRE ROUTING NO.</td>
                    <td>:</td>
                    <td>021000021</td>
                  </tr>
                  <tr>
                    <td>CREDIT TO</td>
                    <td>:</td>
                    <td>ICICI BANK, UDAIPUR</td>
                  </tr>
                  <tr>
                    <td>SWIFT</td>
                    <td>:</td>
                    <td>ICICINBBCTS</td>
                  </tr>
                  <tr>
                    <td>A/C NAME</td>
                    <td>:</td>
                    <td>MEWAR TRADERS & ENGINEERS</td>
                  </tr>
                  <tr>
                    <td>A/C NO.</td>
                    <td>:</td>
                    <td>430405500101</td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class='grid-cols-24 grid w-full border-black'>
          <table style='width: 210mm; table-layout: fixed'>
            <thead>
              <tr>
                <th class='border-t border-black' style='width: 7%'>S.NO</th>
                <th class='border border-black' style='width: 20%'>
                  DESCRIPTION OF GOODS
                </th>
                <th class='border border-black' style='width: 6%'>PO QTY</th>
                <th class='border border-black' style='width: 6%'>UNIT</th>
                <th class='border border-black' style='width: 10%'>
                  PACK NO.
                  <br />
                </th>
                <th class='border border-black' style='width: 12%'>
                  NO. OF PACK
                  <br />
                </th>
                <th class='border border-black' style='width: 12%'>
                  WEIGHT DETAILS
                </th>
                <th class='border border-black' style='width: 12%'>
                  TOTAL WEIGHT
                </th>
                <th class='border border-black' style='width: 12%'>
                  COUNTRY OF ORIGIN
                </th>
                {{!-- <th class='border-t border-black' style='width: 10%'>PO REF</th> --}}
              </tr>
            </thead>
            <tbody>
              {{#each items}}
                <tr>
                  <td class='border-b border-t border-black'>{{i}}</td>
                  <td
                    class='border border-black'
                    style='white-space: normal; word-wrap: break-word'
                  >
                    {{description}}
                  </td>
                  <td
                    class='border border-black text-center'
                    style='white-space: normal; word-wrap: break-word'
                  >
